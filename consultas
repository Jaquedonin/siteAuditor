SELECT  id, fb_user  FROM  alunos;
SELECT  * FROM  alunos
INNER JOIN  professores_escolas
ON alunos.professor_escola_id = professores_escolas.id;

SELECT  id, url  FROM  videos;
SELECT  * FROM  videos
INNER JOIN  professores_escolas
ON videos.professor_escola_id = professores_escolas.id;

SELECT  id FROM  categoria_video;
SELECT  * FROM  categoria_video
INNER JOIN  videos
ON categoria_video.videos_id = videos.id;
SELECT  * FROM  categoria_video
INNER JOIN  categorias
ON categoria_video.categorias_id = categorias.id;

SELECT  id, nome, codigo  FROM  cidades;

SELECT  id, nome, sigla  FROM  escolas;
SELECT  * FROM  escolas
INNER JOIN  cidades
ON escolas.cidade_id = cidades.id;

SELECT  id  FROM  professores_escolas;
SELECT  * FROM  professores_escolas
INNER JOIN  escolas
ON professores_escolas.escola_id = escolas.id;
SELECT  * FROM  professores_escolas
INNER JOIN  professores
ON professores_escolas.professor_id = professores.id;

SELECT  id, descricao, tipo  FROM  categorias;

SELECT  id, fb_user, email, senha  FROM  professores;

SELECT  id, data_hora, turma  FROM  visitas;
SELECT  * FROM  visitas
INNER JOIN professores_escolas
ON visitas.professores_escolas_id = professores_escolas.id;

SELECT  id  FROM  categoria_visita;
SELECT  * FROM  categoria_visita
INNER JOIN visitas
ON categoria_visita.visitas_id = visitas.id;
SELECT  * FROM  categoria_visita
INNER JOIN categorias
ON categoria_visita.categorias_id = categorias.id;

SELECT  id  FROM  categoria_acervo;
SELECT  * FROM  categoria_acervo
INNER JOIN categorias
ON categoria_acervo.categorias_id = categorias.id;
SELECT  * FROM  categoria_acervo
INNER JOIN acervo
ON categoria_acervo.acervo_id = acervo.id;

SELECT  id, arquivo  FROM  acervo;